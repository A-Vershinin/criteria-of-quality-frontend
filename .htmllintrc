{
  "attr-bans": ["align", "background", "bgcolor", "border", "frameborder", "longdesc", "marginwidth", "marginheight", "scrolling"],
  "attr-name-ignore-regex": "/X-UA-Compatible/",
  "attr-name-style": "lowercase",
  "attr-new-line": false,
  "attr-no-dup": true,
  "attr-no-unsafe-char": true,
  "attr-order": ["class"],
  "attr-quote-style": "double",
  "attr-req-value": true,
  "class-no-dup": true,
  "class-style": "bem",
  "doctype-first": true,
  "doctype-html5": true,
  "fig-req-figcaption": false,
  "focusable-tabindex-style": false,
  "head-req-title": true,
  "head-valid-content-model": true,
  "href-style": false,
  "html-req-lang": true,
  "html-valid-content-model": true, // хреновое описание, разобраться
  "id-class-ignore-regex": false,
  "id-class-no-ad": true,
  "id-class-style": "lowercase",
  "id-no-dup": true,
  "img-req-alt": true,
  "img-req-src": true,
  "indent-style": "spaces",
  "indent-width": 2,
  "indent-width-cont": false,
  "input-radio-req-name": false,
  "input-req-label": true,
  "label-req-for": false,
  "lang-style": "case",
  "line-end-style": "lf",
  "line-max-len": false,
  "line-max-len-ignore-regex": false,
  "spec-char-escape": true,
  "table-req-caption": false,
  "table-req-header": false,
  "tag-bans": ["font"],
  "tag-close": true,
  "tag-name-lowercase": true,
  "tag-name-match": false, // хреновое описание, разобраться
  "tag-self-close": "never",
  "title-max-len": 60,
  "title-no-dup": true
}
